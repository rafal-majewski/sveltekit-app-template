	},
	"devDependencies": {
		"@commitlint/cli": "19.6.0",
		"@commitlint/types": "19.5.0",
		"@playwright/test": "1.49.0",
		"@release-it/conventional-changelog": "9.0.3",
		"@stylistic/eslint-plugin": "2.11.0",
		"@sveltejs/adapter-node": "5.2.9",
		"@sveltejs/kit": "2.8.4",
		"@sveltejs/vite-plugin-svelte": "4.0.2",
		"@testing-library/svelte": "5.2.6",
		"@types/node": "22.10.0",
		"@typescript-eslint/eslint-plugin": "8.16.0",
		"@typescript-eslint/parser": "8.16.0",
		"@vitest/coverage-v8": "2.1.5",
		"eslint": "9.15.0",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-perfectionist": "4.1.2",
		"eslint-plugin-sort-destructure-keys": "2.0.0",
		"eslint-plugin-svelte": "2.46.0",
		"jsdom": "25.0.1",
		"postcss-html": "1.7.0",
		"postcss-scss": "4.0.9",
		"prettier": "3.4.0",
		"prettier-plugin-svelte": "3.3.2",
		"release-it": "17.10.0",
		"sass-embedded": "1.81.0",
		"stylelint": "16.10.0",
		"stylelint-order": "6.0.4",
		"stylelint-scss": "6.10.0",
		"svelte": "5.2.8",
		"svelte-check": "4.1.0",
		"svelte-eslint-parser": "0.43.0",
		"svelte-preprocess": "6.0.3",
		"testcontainers": "10.15.0",
		"typescript": "5.7.2",
		"vite": "5.4.11",
		"vitest": "2.1.5",
		"zod": "3.23.8"
	},
	"homepage": "https://github.com/Rafal-Majewski/sveltekit-app-template#readme",
	"license": "CC0-1.0",
	"name": "sveltekit-app-template",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Rafal-Majewski/sveltekit-app-template.git"
	},
	"scripts": {
		"build": "vite build",
		"build:check": "svelte-check --fail-on-warnings --tsconfig ./tsconfig.json",
		"commitlint:check": "commitlint",
		"dev": "vite dev",
		"eslint:check": "eslint --max-warnings 0 .",
		"eslint:fix": "eslint --max-warnings 0 --fix .",
		"formatting:check": "prettier --check .",
		"formatting:fix": "prettier --write .",
		"playwright:check": "playwright test --pass-with-no-tests '.+\\.e2e\\.test\\.ts'",
		"prebuild": "svelte-kit sync",
		"prebuild:check": "svelte-kit sync",
		"predev": "svelte-kit sync",
		"preplaywright:check": "svelte-kit sync",
		"preslint:check": "svelte-kit sync",
		"preslint:fix": "svelte-kit sync",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"preview": "svelte-kit sync && vite preview",
		"release": "release-it",
		"stylelint:check": "stylelint **/*.{scss,svelte}",
		"stylelint:fix": "stylelint --fix **/*.{scss,svelte}",
		"svelte:check": "svelte-kit sync && svelte-check --fail-on-warnings",
		"typescript:check": "svelte-kit sync && tsc --noEmit",
		"vitest:check": "svelte-kit sync && vitest run --passWithNoTests --coverage"
	},
	"type": "module",
	"version": "0.0.0"
}
